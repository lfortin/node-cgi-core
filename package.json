{
  "name": "cgi-core",
  "description": "Minimalistic, zero-dependency wrapper for CGI scripts",
  "version": "0.9.12",
  "homepage": "https://github.com/lfortin/node-cgi-core#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/lfortin/node-cgi-core.git"
  },
  "author": {
    "name": "Laurent Fortin",
    "email": "laurent.fortin@gmail.com"
  },
  "main": "./cgi-core.js",
  "type": "commonjs",
  "types": "./cgi-core.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "cypress": "^13.14.2",
    "mocha": "^10.5.2",
    "mock-os": "^1.0.0",
    "nyc": "^17.0.0",
    "selfsigned": "^2.4.1"
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=text-summary mocha test/**/*.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run --e2e --headless --browser electron",
    "prepublishOnly": "npm test && npm run cy:run",
    "start": "node server.mjs",
    "test": "mocha test/**/*.js"
  },
  "keywords": [
    "cgi",
    "http"
  ],
  "license": "MIT"
}
